@model IEnumerable<Fiap05.Web.MVC.Models.CartaoReal>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/CartaoReal/Cadastrar.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Numero)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cvv)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Numero)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cvv)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CartaoRealId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CartaoRealId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CartaoRealId })
        </td>
    </tr>
}

</table>
